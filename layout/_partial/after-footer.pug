if theme.ga.active
  script(async="",src="https://www.googletagmanager.com/gtag/js?id=" + theme.ga.id)
  script.
    window.dataLayer = window.dataLayer || [];
    function gtag() {
      dataLayer.push(arguments);
    }
    gtag('js', new Date());

    var metas = document.getElementsByTagName('meta');
    var gaMeta = Array.from(metas).find((meta) => meta.name === 'ga');

    gaMeta && gtag('config', gaMeta.content);
